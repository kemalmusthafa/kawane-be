{
  "name": "kawane-studio-backend",
  "version": "1.0.0",
  "description": "Backend API for Kawane Studio e-commerce platform",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc && node scripts/copy-templates.js",
    "build:win": "npx tsc && xcopy /E /I /Y src\\templates dist\\templates",
    "build:linux": "npx tsc && node scripts/copy-templates.js",
    "start": "npm run build:win && node dist/index.js",
    "start:prod": "NODE_ENV=production npm run build && node dist/index.js",
    "start:vercel": "NODE_ENV=production npm run build:linux && node dist/index.js",
    "dev": "ts-node-dev src/index.ts",
    "dev:prod": "NODE_ENV=production ts-node-dev src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "prisma generate",
    "migrate:deploy": "prisma migrate deploy",
    "vercel-build": "npm run build:linux && npm run migrate:deploy",
    "seed": "ts-node prisma/seed.ts",
    "seed:users": "ts-node prisma/user-seed.ts",
    "db:seed": "npm run seed",
    "db:push": "npx prisma db push",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "midtrans:dev": "MIDTRANS_IS_PRODUCTION=false npm run dev",
    "midtrans:prod": "MIDTRANS_IS_PRODUCTION=true npm run dev"
  },
  "keywords": [
    "e-commerce",
    "api",
    "nodejs",
    "express",
    "typescript",
    "prisma",
    "postgresql"
  ],
  "author": "Kawane Studio",
  "license": "MIT",
  "type": "commonjs",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/midtrans-client": "^1.4.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.0",
    "@types/nodemailer": "^7.0.1",
    "@types/streamifier": "^0.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@types/ioredis": "^4.28.10",
    "@types/node-cron": "^3.0.11",
    "autoprefixer": "^10.4.21",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "cloudinary": "^2.7.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "express-rate-limit": "^8.0.1",
    "google-auth-library": "^10.3.0",
    "handlebars": "^4.7.8",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "midtrans-client": "^1.4.3",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "node-fetch": "^2.7.0",
    "nodemailer": "^7.0.5",
    "postcss": "^8.5.6",
    "prisma": "^6.14.0",
    "streamifier": "^0.1.1",
    "tailwindcss": "^3.4.18",
    "zod": "^4.1.5"
  }
}